<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="kr.yme.mapper.MemberMapper">
	<insert id="insertMember" parameterType="kr.yme.entity.Member">
		insert into t_member values(#{id},#{pw},#{name},#{nick},#{phone},#{univ},#{circle_from},#{preference},#{region},#{mbti},#{birthdate},now())
	</insert>
</mapper>

